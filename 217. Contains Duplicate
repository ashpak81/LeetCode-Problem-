class Solution {
    public boolean containsDuplicate(int[] nums) {
        Arrays.sort(nums);
        boolean ans = false;
        int n = nums.length;
        for(int i = 0 ;i<n;i++){
           if(i<n-1){
            int j = i+1;
            if(nums[i]==nums[j]){
                ans = true;
            }
        }
    }
    return ans;
    }
}
